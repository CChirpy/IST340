{
    "sass.enabled": false,
    "sass.options": {
        "includePaths": [
            "website/scss/"
        ],
        "outputDir": "../css/",
        "imagePath": null,
        "sourceComments": false,
        "outputStyle": "nested"
    },
    "linting.collapsed": true,
    "spaceUnits": 3,
    "path": {
        "website/scss/styles.scss": {
            "sass.enabled": true,
            "useTabChar": false,
            "spaceUnits": 3,
            "linting.collapsed": false
        },
        "website/scss/layout.scss": {
            "sass.enabled": true,
            "useTabChar": false,
            "spaceUnits": 3,
            "linting.collapsed": false,
            "wordWrap": false
        }
    }
}